{
  "name": "flex-template",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "npm-run-all --parallel dev:frontend dev:backend",
    "dev:frontend": "vite --open",
    "dev:backend": "convex dev",
    "build": "vite build",
    "lint": "tsc -p convex -noEmit --pretty false && tsc -p . -noEmit --pretty false && convex dev --once && vite build",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "test:visual": "playwright test",
    "test:visual:update": "playwright test --update-snapshots"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.980.0",
    "@aws-sdk/s3-request-presigner": "^3.980.0",
    "@convex-dev/auth": "^0.0.90",
    "@convex-dev/r2": "^0.9.0",
    "@convex-dev/resend": "^0.2.3",
    "@descope/react-sdk": "^2.26.2",
    "@tailwindcss/postcss": "^4.1.18",
    "@types/js-cookie": "^3.0.6",
    "@vercel/speed-insights": "^1.3.1",
    "@vis.gl/react-google-maps": "^1.7.1",
    "axios": "^1.13.4",
    "browser-image-compression": "^2.0.2",
    "cheerio": "^1.2.0",
    "clsx": "^2.1.1",
    "convex": "^1.31.7",
    "date-fns": "^4.1.0",
    "framer-motion": "^12.29.2",
    "js-cookie": "^3.0.5",
    "leaflet": "^1.9.4",
    "lucide-react": "^0.563.0",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "react-lazy-load-image-component": "^1.6.3",
    "react-leaflet": "^5.0.0",
    "react-markdown": "^10.1.0",
    "react-router-dom": "^7.13.0",
    "remark-gfm": "^4.0.1",
    "sonner": "^2.0.3",
    "tailwind-merge": "^3.1.0",
    "web-push": "^3.6.7"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@playwright/test": "^1.58.2",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.2",
    "@types/node": "^25.2.2",
    "@types/react": "^19.2.13",
    "@types/react-dom": "^19.2.3",
    "@types/react-lazy-load-image-component": "^1.6.5",
    "@types/web-push": "^3.6.4",
    "@vitejs/plugin-react-swc": "^4.2.3",
